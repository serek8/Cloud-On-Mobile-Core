{
    "version": "0.2.0",
    "configurations": [
    
        {
            "name": "Launch (lldb)",
            "type": "lldb",
            "request": "launch",
            "program": "${workspaceFolder}/build/CloudOnMobileCoreClient",
            "internalConsoleOptions": "openOnSessionStart",
            "logging": {
